page{
  background-color: #eee;
}
/* 搜索框 */
.search{
  width: 100%;
  height: 100rpx;
  background-color: #ff6801;
}
.searchwin{
  margin:0 auto;
  box-sizing: border-box;
  width: 85%;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  border: 1px solid #fff;
  border-radius: 50rpx;
  padding-left: 40rpx;
  padding-right: 20rpx;
  margin-top: 15rpx;
  align-items: center;
  align-self: center;
  justify-content: space-between;
  background-color: #fff;
}
.historysearch{
  width: 100%;
  height: 300rpx;
  background-color: #fff;
}
.histitle{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}
.hisdelete{
  width: 40rpx;
  height: 40rpx;
}
.his{
  margin-top: 30rpx;
}
.histips{
  color: #555;
  font-size: 28rpx;
  background-color: #eee;
  padding: 10rpx;
  margin: 30rpx;
  border-radius: 10rpx;
}